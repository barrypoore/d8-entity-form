{#
/**
 * @file marquee.html.twig
 * Default theme implementation to present Marquee data.
 *
 * This template is used when viewing Marquee pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_marquee()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('marquee') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
